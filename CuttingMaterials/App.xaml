<Application x:Class="CuttingMaterials.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CuttingMaterials"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View\Resources\ButtonIcons\Open.xaml"/>
                <ResourceDictionary Source="View\Resources\ButtonIcons\Create.xaml"/>
                <ResourceDictionary Source="View\Resources\ButtonIcons\Save.xaml"/>
                <ResourceDictionary Source="View\Resources\ButtonIcons\Print.xaml"/>
                <ResourceDictionary Source="View\Resources\ButtonIcons\Details.xaml"/>
                <ResourceDictionary Source="View\Resources\ButtonIcons\Offcut.xaml"/>
                <ResourceDictionary Source="View\Resources\ButtonIcons\Pdf.xaml"/>
                <ResourceDictionary Source="View\Resources\ButtonIcons\Template.xaml"/>
                <ResourceDictionary Source="View\Resources\ButtonIcons\ManualEdit.xaml"/>
                <ResourceDictionary Source="View\Resources\ButtonIcons\ClearAll.xaml"/>
                <ResourceDictionary Source="View\Resources\Validation.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
